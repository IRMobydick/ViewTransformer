<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pyxis.uzuki.live.viewtransformer.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".java.TextViewActivity" />
        <activity android:name=".kotlin.TextViewActivity" />
        <activity android:name=".java.EditTextActivity" />
        <activity android:name=".kotlin.EditTextActivity" />
        <activity android:name=".kotlin.CompoundButtonActivity" />
        <activity android:name=".java.CompoundButtonActivity" />
        <activity android:name=".java.ButtonActivity" />
        <activity android:name=".kotlin.ButtonActivity" />
        <activity android:name=".java.ImageViewActivity" />
        <activity android:name=".kotlin.ImageViewActivity" />
    </application>

</manifest>